// Page Sidebar Start
- var status_toggle = (sidebar_open ? undefined : 'checked');
.sidebar-wrapper
  div
    .logo-wrapper
      a(href='index.html')
        img.img-fluid.for-light(src='../assets/images/logo/logo.png', alt='logo')
        img.img-fluid.for-dark(src='../assets/images/logo/logo-dark.png', alt='logo-dark')
      .back-btn
        i.fa.fa-angle-left
      .toggle-sidebar
        i.status_toggle.middle(data-feather='align-left').sidebar-toggle 
    .logo-icon-wrapper
      a(href="index.html")
        img.img-fluid.for-light(src='../assets/images/logo/logo-icon.png', alt='logo-icon')
        img.img-fluid.for-dark(src='../assets/images/logo/logo-icon-dark.png', alt='logo-icon-dark')
    nav.sidebar-main
      .left-arrow#left-arrow
        i(data-feather='arrow-left')
      #sidebar-menu
        ul.sidebar-links#simple-bar
          li.back-btn
            .mobile-back.text-end
              span Back
              i.fa.fa-angle-right.ps-2(aria-hidden='true')
          li.sidebar-list
            a.sidebar-link.sidebar-title(href='../template/index.html', target='_blank')
              i(data-feather='home')
              span Dashboard
          li.sidebar-list
            a.sidebar-link.sidebar-title(href='javascript:void(0)')
              i(data-feather='anchor')
              span Starter kit
            ul.sidebar-submenu
              li
                a.submenu-title(href='javascript:void(0)')
                  | Color Version
                  span.sub-arrow
                    i.fa.fa-chevron-right
                ul.nav-sub-childmenu.submenu-content
                  li
                    a(href='index.html') Layout Light
                  li
                    a(href='layout-dark.html') Layout Dark
              li      
                a.submenu-title(href='javascript:void(0)')
                  | Page Layout
                  span.sub-arrow
                    i.fa.fa-chevron-right
                ul.nav-sub-childmenu.submenu-content
                  li
                    a(href='boxed.html') Boxed
                  li
                    a(href='layout-rtl.html') RTL                
              li      
                a.submenu-title(href='javascript:void(0)')
                  | Footers
                  span.sub-arrow
                    i.fa.fa-chevron-right
                ul.nav-sub-childmenu.submenu-content
                  li
                    a(href='footer-light.html') Footer Light
                  li
                    a(href='footer-dark.html') Footer Dark
                  li
                    a(href='footer-fixed.html') Footer Fixed
          li.sidebar-list
            a.sidebar-link.sidebar-title(href='https://pixelstrap.freshdesk.com/support/home', target='_blank')
              i(data-feather='headphones')
              span Raise Support
          li.sidebar-list
            a.sidebar-link.sidebar-title(href='../documentation/document/index.html', target='_blank')
              i(data-feather='file-text')
              span Documentation                                    
        .sidebar-img-section
          .sidebar-img-content
            img(src="../assets/images/dashboard/upgrade/2.png", alt="").img-fluid                
            h4 Experiance with more Features
            a.btn.btn-primary(href="https://themeforest.net/user/pixelstrap/portfolio") Check now
      .right-arrow#right-arrow
        i(data-feather='arrow-right')
// Page Sidebar Ends
